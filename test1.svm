struct Vector3{
	float x;
	float y;
	float z;
};

int Summation(int n){
	int total = 0;
	int counter = 1;
	while(counter < n+1){
		total = total + counter;
		counter = counter + 1;
	}
	
	return total;
}

int Factorial(int x){
	if(x < 2){
		return 1;
	}
	
	return x * Factorial(x-1);
}

